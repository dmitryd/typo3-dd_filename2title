{
    "name": "dmitryd/typo3-dd-filename2title",
    "type": "typo3-cms-extension",
    "description": "Sets file title to the file name on upload if no other metada extractor came with anything more useful. See https://forge.typo3.org/issues/61106",
    "homepage": "https://github.com/dmitryd/typo3-dd_filename2title",
    "version": "1.0.0",
    "time": "2016-09-28",
    "license": "GPL-3.0+",
    "keywords": [
        "TYPO3",
        "CMS",
        "typo3",
    ],
    "replace": {
        "dd_filename2title": "self.version",
        "typo3-ter/dd-filename2title": "self.version"
    },
    "require": {
        "typo3/cms-core": ">=6.2.0,<=9.0.0",
        "php": ">=5.6.0"
    },
    "autoload": {
        "psr-4": {
            "DmitryDulepov\\Filename2Title\\": "Classes"
        }
    },
    "authors": [
        {
            "name": "Dmitry Dulepov",
            "email": "dmitry.dulepov@gmail.com",
            "homepage": "http://www.dmitry-dulepov.com/"
        }
    ],
}
